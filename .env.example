# API Configuration
API_URL=https://www.easydoct.com/api/rdv/getRdvDayAvailabilities

# Automated Login Configuration
# Set to 'true' to enable automated login (requires EASYDOCT_EMAIL, EASYDOCT_PASSWORD, EXAM_URL)
# Set to 'false' to use manual session cookies (requires SESSION_KEY, USER_SESSION_KEY, ASPNET_COOKIES)
AUTO_LOGIN_ENABLED=true

# Easydoct Login Credentials (REQUIRED if AUTO_LOGIN_ENABLED=true)
EASYDOCT_EMAIL=your.email@example.com
EASYDOCT_PASSWORD=your_password_here
EXAM_URL=https://www.easydoct.com/rdv/gie-irldr-imagerie-rennes

# Session Cookies (REQUIRED if AUTO_LOGIN_ENABLED=false, auto-generated if true)
# These will be automatically obtained if AUTO_LOGIN_ENABLED=true
# Otherwise, obtain them manually from browser session
SESSION_KEY=your_session_key_here
USER_SESSION_KEY=N
ASPNET_COOKIES=your_aspnet_cookies_here

# Appointment Parameters (REQUIRED)
EXAM_TYPE_ID=3374
EXAM_ID=56794
PATIENT_BIRTH_DATE=1950-01-101T22:00:00.000+02:00

# Polling Configuration
POLL_INTERVAL_SECONDS=60

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=irm_slots.log

# Notification Configuration
NOTIFICATIONS_ENABLED=true
SLACK_TOKEN=xoxb-your-slack-bot-token
SLACK_CHANNEL_ID=C01234567890
